integer numStrokes

numStrokes = Get next input

// Assumes "pat 4"
if numStrokes <= 0
   Put "Invalid entry." to output
elseif numStrokes == 1
   Put "Hole in 1!!!" to output
elseif numStrokes == 2
   Put "Eagel!" to output
elseif numStrokes == 3
   Put "Birdie" to output
elseif numStrokes == 4
   Put "Par" to output
else
   Put "Better luck next time." to output